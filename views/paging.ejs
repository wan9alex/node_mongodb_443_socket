<% if(page_data.length!=0){ %>
<nav aria-label="...">
    <ul class="pagination small">
      
      <li class="page-item <%= start==1 ? 'disabled' : '' %>">
        <a class="page-link" href="<%= '/admin/' + api_name + '?dataName=' + dataName + '&start=' + (start-1) + '&q=' + q + '&rule=' + rule %>">上一页</a>
      </li>
      
      <% for(var i=1; i<=page_count; i++){ %>
      <li class="page-item <%= start==i ? 'active' : '' %>">
        <a class="page-link" href="<%= '/admin/' + api_name + '?dataName=' + dataName + '&start=' + i + '&q=' + q + '&rule=' + rule %>"><%= i %></a>
      </li>
      <% } %>

      <li class="page-item <%= start==page_count ? 'disabled' : '' %>"">
        <a class="page-link" href="<%= '/admin/' + api_name + '?dataName=' + dataName + '&start=' + (start+1) + '&q=' + q + '&rule=' + rule %>">下一页</a>
      </li>
      
    </ul>
  </nav>
  <% } %>